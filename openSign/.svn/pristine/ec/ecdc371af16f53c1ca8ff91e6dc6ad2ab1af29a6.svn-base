#gnb .gnb_menu li .menu02 span{background:url(../images/map/i_gnb02.png) no-repeat 50% 30%;}
#gnb .gnb_menu li .menu03 span{background:url(../images/map/i_gnb03.png) no-repeat 50% 30%;}
#gnb .gnb_menu li .menu04 span{background:url(../images/map/i_gnb04.png) no-repeat 50% 30%;}

#gnb .gnb_menu li.active .menu01 span, #gnb .gnb_menu li:hover .menu01 span{background:url(../images/map/i_gnb01_on.png) no-repeat 50% 30%;}
#gnb .gnb_menu li.active .menu02 span, #gnb .gnb_menu li:hover .menu02 span{background:url(../images/map/i_gnb02_on.png) no-repeat 50% 30%;}
#gnb .gnb_menu li.active .menu03 span, #gnb .gnb_menu li:hover .menu03 span{background:url(../images/map/i_gnb03_on.png) no-repeat 50% 30%;}
#gnb .gnb_menu li.active .menu04 span, #gnb .gnb_menu li:hover .menu04 span{background:url(../images/map/i_gnb04_on.png) no-repeat 50% 30%;}


#side_content{position:relative;float:left; width:360px; height:100%; overflow:hidden; background:#fff; box-shadow:0px 0px 0px 0px rgba(0,0,0,0.2); transition:width 0.1s;}
#map_wrap.side_close #side_content {width:0;}
.side_scl_box {position: absolute;top: 0px;right: 0;bottom: 0;width: 100%; overflow: hidden;}
.side_scl {position: relative; width: 100%; height: 100%; overflow-y: auto;}
.map_search_area {padding:20px 20px 0px;}
.map_search_area .title {padding:0 0 13px 26px;display:block; float:left; width:55%; color:#23278c;font-size:18px; background:url(../images/map/i_subway_tit.png) no-repeat 0 0;}

.search_option select {height:45px; border:1px solid #f2f2f2; background-image:url(../images/map/bg_selc.png); background-position:90% 50%; background-repeat:no-repeat;}
.search_option .selc01 {width:56%;}
.search_option .selc02 {margin-left:9px;width:40%;}
.search_input {margin-top:6px;background:#f2f2f2; padding:5px; height:45px; border-radius: 4px;}
.search_input input[type=text] {padding-left:10px;background:transparent; border:0; width:calc(100% - 50px)}
.search_input input[type=text]::placeholder {color:#c5c6c7; font-weight:400; font-size:15px;}
.search_input .btn_search {text-indent:-9999px;width:40px; height:100%; background:url(../images/map/bg_search.png) no-repeat 50% 50%;}

.btn_allsearch {background-color:#3898fe; border-color:#3898fe;}

.map_current {padding:20px 20px 0px;}
.gps_place {width:100%;}
.gps_place .top_box {display:table; width:100%; }
.gps_place .lt {display:table-cell; width:60%; vertical-align:top;}
.gps_place .lt .title {padding-left:32px;font-size:18px; color:#444; letter-spacing:-0.05em; background:url(../images/map/bg_place.png) no-repeat 0 0; vertical-align:top;}
.gps_place .rt {display:table-cell; width:40%; vertical-align:top; text-align:right;}
.weather_box .tit {display:inline-block;font-size:13px; color:#888; letter-spacing:-0.06em;}
.weather_box .txt {display:inline-block; margin-left:5px;font-size:18px; color:#444; letter-spacing:-0.06em; font-weight:bold;}

.dust_box {margin-top:5px; padding-bottom:20px; width:100%; text-align:right; border-bottom:1px solid #e8e8e8}
.dust_box > span {display:inline-block; margin-right:5px;}
.dust_box > span:last-child {margin-right:0;}
