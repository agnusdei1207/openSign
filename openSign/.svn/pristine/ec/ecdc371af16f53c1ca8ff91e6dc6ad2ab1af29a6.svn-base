.wireless_wrap .result_box .tit {position:relative;margin-right:5px;color:#888; font-size:14px;}
.wireless_wrap .result_box .tit:after{position:absolute; top:50%; left:0; background:#888; width:2px; height:2px; content:'';}
.wireless_wrap .result_box .txt {color:#888; font-size:14px; margin-right:5px;}
.wireless_wrap .result_box .num {position:relative;color:#000; font-size:14px; padding-left:5px;}
.wireless_wrap .result_box .num:after {display:none;}

.subway_result {position:relative; margin-top:20px;}
.subway_result .result_top .title {border-bottom:1px solid #e8e8e8;}
.subway_result .tbl_scl {top:40px;padding:0 20px;}

.tbl_scoll_wrap {position: relative;padding:29px 20px 0;}
.tbl_scoll_box {width: 100%;margin-top:8px;overflow-x: hidden;overflow-y: auto; height:515px;}

.subway_tbl {table-layout: fixed;border-top: 1px solid #000;font-size: 15px;}
.subway_tbl .w180 {width:180px;}
.subway_tbl .w140 {width:140px;}
.subway_tbl thead tr{position: absolute;left: 20px;top: 0;height: 30px; border-top:1px solid #e1e1e1;}
.subway_tbl thead tr:last-child {bottom: 0;}
.subway_tbl thead th {padding:8px 0;text-align:center;color: #31343f;border-bottom: 1px solid #e1e1e1;border-right: 1px solid #e1e1e1;font-weight:600;background: #e7eff9;}
.subway_tbl tbody th {padding:8px 10px;color: #31343f;border-bottom: 1px solid #e1e1e1;border-right: 1px solid #e1e1e1;font-weight:500;background: #e7eff9;}
.subway_tbl tbody td {position: relative; padding:8px 5px;text-align:center;color: #222;border-bottom: 1px solid #e1e1e1;border-right: 1px solid #e1e1e1;font-weight: 300;}
.subway_tbl tbody td a {display:block; width:100%;}
.subway_tbl thead th:last-child,
.subway_tbl tbody td:last-child {border-right:0;}
.subway_tbl tbody td.r {padding:12px 10px;text-align: right;}
.subway_tbl tbody td.no_data {color:#999;}
.subway_tbl tbody tr.active {background:#f4f9ff}
.subway_tbl tbody tr:hover {background:#f4f9ff}

.tbl_col_type01 {table-layout: fixed; font-size: 13px}
