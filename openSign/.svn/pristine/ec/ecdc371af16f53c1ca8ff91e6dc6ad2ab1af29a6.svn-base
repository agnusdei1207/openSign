.bookmark_box {margin-top:12px; padding:0 20px;}
.btn_book {min-width:150px;line-height:35px; border:1px solid #c2dfff; color:#3396ff; font-size:13px;}
.btn_book > span {padding-right:20px;background:url(../images/map/bg_bookmark.png) no-repeat 100% 50%}
.btn_book.on > span {background:url(../images/map/bg_bookmark_on.png) no-repeat 100% 50%}

.result_top {padding:0px 20px 20px;}
.result_wrap {position:relative; height:490px; margin-top:13px; margin-bottom:20px}
.result_scl {position: absolute;top: 95px;right: 0;bottom: 0; padding-right:20px;width: 100%; overflow: hidden;}
.result_top .title {padding:0 0 13px 26px;color:#23278c;font-size:18px;background:url(../images/map/i_search.png) no-repeat 0 10%; }
.result_box {padding:13px 0;border:1px solid #e8e8e8;border-left:0; border-right:0;}
.result_box .tit {margin-right:5px;color:#258fff; font-size:14px;}
.result_box .txt {color:#888; font-size:14px; margin-right:5px;}
.result_box .num {position:relative;color:#000; font-size:14px; padding-left:5px;}
.result_box .num:after {position:absolute; top:3px; left:0; content:''; width:1px; background:#b8b7b7; height:14px;}

.result_list {position: relative; width: 100%; height: 100%; overflow-y: auto;}
.result_list li {padding:0 10px;}
.result_list li.active {background:#f4f9ff}
.result_list li:hover {background:#f4f9ff}
.result_list li a{display:block;position:relative;margin-left:10px;padding:18px 0 18px 40px; border-bottom:1px solid #e7e8e9;}
.result_list li a .num {position:absolute; top:18px; left:0;width:35px; height:25px; background:#fff; text-align:center; line-height:22px; border:1px solid #258fff; color:#258fff; font-size:13px; font-weight:600; border-radius:25px;}
.result_list li a .tit {display:inline-block;color:#333; font-size:16px; font-weight:600; width:80%; letter-spacing:-0.05em; line-height:1.3; overflow:hidden; text-overflow:ellipsis;white-space:nowrap;}
.result_list li a .txt {display:inline-block; color:#888; font-size:14px; width:80%; letter-spacing:-0.06em; line-height:1.3;}
.result_list li a .point {position:absolute; top:18px; right:0;width:30px; height:30px;background:url(../images/map/bg_mappin.png) no-repeat 0 0;}

