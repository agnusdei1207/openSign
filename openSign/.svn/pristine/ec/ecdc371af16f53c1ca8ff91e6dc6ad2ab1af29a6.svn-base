.tbl_col_type01 tbody td {padding:4px 0; text-align:center; border-bottom:1px solid #e8e8e8; border-left:0; border-right:0;}

.tbl_col_type02 {table-layout: fixed; font-size: 14px; }
.tbl_col_type02 thead th {padding:4px 0; text-align:center; border:1px solid #258fff; border-left:0; border-right:0; color:#258fff;}
