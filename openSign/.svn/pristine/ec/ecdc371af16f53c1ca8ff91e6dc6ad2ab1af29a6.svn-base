  font-family: 'NotoSansKR';
  font-style: normal;
  font-weight: 300;
  src: url(../font/NotoSansKR-Light.woff2) format('woff2'), url(../font/NotoSansKR-Light.woff) format('woff'), url(../font/NotoSansKR-Light.otf) format('opentype');
}
@font-face {
   font-family: 'NotoSansKR';
   font-style: normal;
   font-weight: 400;
   src: url(../font/NotoSansKR-Regular.woff2) format('woff2'), url(../font/NotoSansKR-Regular.woff) format('woff'), url(../font/NotoSansKR-Regular.otf) format('opentype');
 }
@font-face {
   font-family: 'NotoSansKR';
   font-style: normal;
   font-weight: 600;
   src: url(../font/NotoSansKR-Medium.woff2) format('woff2'), url(../font/NotoSansKR-Medium.woff) format('woff'), url(../font/NotoSansKR-Medium.otf) format('opentype');
 }
@font-face {
   font-family: 'NotoSansKR';
   font-style: normal;
   font-weight: 700;
   src: url(../font/NotoSansKR-Bold.woff2) format('woff2'), url(../font/NotoSansKR-Bold.woff) format('woff'), url(../font/NotoSansKR-Bold.otf) format('opentype');
 }
@font-face {
   font-family: 'NotoSansKR';
   font-style: normal;
   font-weight: 800;
   src: url(../font/NotoSansKR-Black.woff2) format('woff2'), url(../font/NotoSansKR-Black.woff) format('woff'), url(../font/NotoSansKR-Black.otf) format('opentype');
 }
 
html, body {width: 100%;height:100%;margin:0;padding:0;}
body {word-spacing:-1px;color:#333333;font-family:'NotoSansKR', 'malgun gothic', 'dotum';font-size:14px;font-weight: 400;line-height:1.4;}
*,
*:after,
*:before {box-sizing: border-box;}
img {display:inline-block;border:0 none;font-size: 0;line-height:1;}
cite, code, dfn, del, em, ins, label, q, span, strong {display:inline-block;}
cite, em, dfn {font-style:normal;}
sub {vertical-align:bottom;}
sup {vertical-align:top;}
hr {display:none;}
h1, h2, h3, h4, h5, h6 {margin:0;font-size:1em;}
address {font-style:normal;}
div {box-sizing:border-box;}
p  {box-sizing:border-box;margin:0;}
pre {margin:0;font-family:verdana;font-size:1em;}
table {width:100%;border-spacing:0;border:0;border-collapse:separate;font-family:inherit;}
caption {display:none;}
th, td {padding:0;text-align:left;vertical-align:middle;font-size:inherit;font-weight:normal;}
dd, dl, ol, ul {margin:0;padding:0;}
li {list-style-type:none;}
form {margin:0;}
fieldset {margin:0;padding:0;border:none;}
legend {display:none;}
textarea {border:1px solid #cccccc;font-family:inherit;}
input[type="text"],
input.text {height:36px;padding:0 0 0 5px;vertical-align:middle;color:inherit;border:1px solid #cccccc;background:#fff;font-family:inherit;font-size:inherit;line-height:1.4;}
input[type="checkbox"],
input[type="radio"] {width:13px;height:13px;margin:0px 3px 1px 0;padding:0;vertical-align:middle;}
select {height:34px;margin:0;padding: 0 25px 0 .5em; vertical-align:middle;border:1px solid #dedede;border-radius: 4px;background:#f2f2f2 url(../images/maps/bg_select.png) no-repeat right 5px top 50%;font-family: inherit;font-size:inherit;-webkit-appearance: none;-moz-appearance: none; appearance: none; }
select::-ms-expand {display: none;}
button {display:inline-block;margin:0;padding:0;cursor:pointer; text-align: center;vertical-align: middle;white-space:nowrap;font-family:inherit;}
a {text-decoration:none;color:inherit; }
a.btn:focus,
input.btn:focus,
button.btn:focus {outline: none;}

.hidden {position:absolute;top:-9000px;left:-9000px;visibility:hidden;}
.display_none {display:none;}
.l {text-align:left !important;}
.c {text-align:center !important;}
.r {text-align:right !important;}
.t {vertical-align:top !important;}
.m {vertical-align:middle !important;}
.b {vertical-align:bottom !important;}
.fl {float:left !important;}
.fr {float:right !important;}
.clear::after {display:table;clear:both;content:'';}

.btn {display: inline-block;cursor: pointer;text-align: center;vertical-align: middle;white-space: nowrap;color:#fff;border:1px solid transparent;border-color: rgba(0, 0, 0, 0);border-radius: 5px;font-family:inherit;font-size: inherit;font-weight: 400;line-height: 1;}
.btn_area {position: relative;margin-top: 20px;text-align: center;}
.btn_big {min-width:120px;height: 38px;font-size: 15px;line-height: 38px;}

/*margin*/
.mar_t0 {margin-top: 0 !important;}
.mar_t3 {margin-top: 3px !important;}
.mar_t5 {margin-top: 5px !important;}
.mar_t15 {margin-top: 15px !important;}

.w100p {width:100% !important;}

/*map*/
#map_wrap {position: relative;min-width: 1280px;height: 100%; min-height: 760px;}
#map_wrap.side_close {}
#map_container {position:relative;width:100%;height:100%;overflow: hidden;}
#map_container:after {display:table; clear:both; content:'';} 


#side_menu {position:relative;float:left;width:70px; height:100%;border-right:1px solid #d9d9d9}
#side_menu .logo {display:block;width:100%;height:68px; line-height:68px; text-align:center;}
#side_menu .logo img {vertical-align:middle;}

.explain {position:absolute; bottom:0; width:100%;}
.explain a {display:block; height:50px; text-align:center; line-height:50px; letter-spacing:-0.05em; font-size:13px; color:#333; background:#f9f9f9; border-top:1px solid #d9d9d9;}
.tool_content {position:absolute; top:0; left:75px; padding:15px; width:300px; z-index:10; background:#fff; border:1px solid #999;}
.tool_content::before {position:absolute; top:25%;left:-11px; z-index:22;content:''; width:19px; height:17px; background:url(../images/map/img_toolArrow.png) no-repeat left top;}

#exp02::before {position:absolute; top:54%;left:-11px; z-index:22;content:''; width:19px; height:17px; background:url(../images/map/img_toolArrow.png) no-repeat left top;}

#exp03 {position:absolute; top:auto; bottom:0; padding:15px; width:300px; z-index:10; background:#fff; border:1px solid #999;}
#exp03::before {position:absolute; top:75%;left:-11px; z-index:22;content:''; width:19px; height:17px; background:url(../images/map/img_toolArrow.png) no-repeat left top;}


#gnb .gnb_menu li{height:90px; border-top:1px solid #d9d9d9;}
