.wireless_wrap .top_box .range_box .wire_selc {min-width:80px;height:28px; color:#222; font-size:13px; background:#fff; background-image: url(../images/map/bg_selc.png); background-position: 90% 50%; background-repeat: no-repeat;}

.result_list02 {position: relative; width: 100%; height: 100%; overflow-y: auto;}
.result_list02 li {padding:0 10px;}
