	.top_slide {position:relative; width:100%;}
	.top_slide_box {position:relative; width:100%;}
	.top_slide_box .slick-prev {display:inline-block;position:absolute;left:0;top:50%;margin-top:-40px; cursor:pointer;text-indent:-9999em; width:38px;height:80px;background:url(../images/main/main_slider_prev.png) no-repeat 0 0;z-index:15;}
	.top_slide_box .slick-next {display:inline-block;position:absolute;right:0;top:50%;margin-top:-40px; cursor:pointer; text-indent:-9999em; width:38px;height:80px;background:url(../images/main/main_slider_next.png) no-repeat 0 0;z-index:15;}
	/* .top_item  {width:100%;} */
	.top_item .item {width:100%;}
	.top_item .item .base {display:block;}
	.top_item .item .base_prev {display:none;}
	.top_item .item img {display:block;width:100%;max-height:161px;}	
	
	.swiper_page_box {position:absolute; width:40px; height:100%; top:10%; margin-left: -107px;left:370px !important;}
	.top_swiper .swiper-pagination {position:relative;width:100%; height:100%;}
	.top_swiper .swiper-pagination:after {display:block;content:''; position:absolute; top:0; left:50%; width:1px; height:100%; background:#d9d9d9; z-index:-1;}
	.top_swiper .swiper-pagination .swiper-pagination-bullet {margin-bottom:30px;font-size:16px;width:40px; height:40px; line-height:40px; font-family:'gotham'; background:#d9d9d9; opacity:1; font-weight:600; color:#fff; }
	.top_swiper .swiper-pagination .swiper-pagination-bullet-active{background:#4b4fc5;}
	
	.bot_slide_box {position: absolute;z-index:2;top:21rem;overflow: hidden;width: 100%;height:26rem;background: #fff;display:none;}
	.bot_slide {width:100%;}
	.bot_slide > img {max-width:100%; height:100%;}
	.bot_slide_box .slick-prev {position:absolute; z-index:3; top:50%; left:1.5rem; width:4rem; height:4rem;text-indent:-9999em;background:url(../images/main/btn_slide_left.png) no-repeat 50% 50%;}
	.bot_slide_box .slick-next {position:absolute; z-index:3; top:50%; right:1.5rem; width:4rem; height:4rem;text-indent:-9999em;background:url(../images/main/btn_slide_right.png) no-repeat 50% 50%;}
	
	.ect_slide_box {position:absolute; bottom:0; left:5%; z-index:3;}
	.visual_slide .slide_paging {position:relative;width:7rem;min-height:3rem;}
	.visual_slide .slide_paging::after {content: '';position: absolute;top:71%;left:50%;width: 14px;height: 0.1rem;transform:translate(-50%) rotate(-75deg);background: #c2c2c2;}
	.visual_slide .slide_paging span {position:absolute;font-size: 16px;color: #c2c2c2;font-weight: 300;line-height: 1.6rem; font-family:'Arial';}
	.visual_slide .slide_paging .count {left:0; top:45%; font-weight:800; font-size:1.8rem; letter-spacing:-0.05em; color:#fff;}
	.visual_slide .slide_paging .pagecount {right:5px; font-weight:700;top:52%;}
	
	/* progress bar */
	.progress_box {position:relative;display:inline-block;width:10rem;height:3rem;}
	.visual_slide .slider-progress {width: 95%;height: 2px;background:#dadada; position: absolute;bottom: 20%;right: 0;}
	.visual_slide .slider-progress .progress {width: 0%;height: 2px;background: #4b4fc5;}
	
	.slider-btn {display:inline-block;vertical-align:5px;}
	.slider-btn #slick_pause {display:inline-block; width:30px; height:30px; text-indent:-9999em; background: url(../images/main/i_pause.png) no-repeat 50% 50%;}
	.slider-btn #slick_play {display:inline-block; width:30px; height:30px; text-indent:-9999em; background: url(../images/main/i_play.png) no-repeat 50% 50%;}
	
	
